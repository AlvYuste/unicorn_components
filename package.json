    "@angular/platform-browser": "4.x",
    "@angular/platform-browser-dynamic": "4.x",
    "@angular/router": "4.x",
    "codelyzer": "3.x",
    "core-js": "2.4.x",
    "moment": "2.18.1",
    "rxjs": "5.x",
    "ts-node": "3.x",
    "tslint": "5.x",
    "typescript": "2.x",
    "chart.js": "2.4.x",
    "autoprefixer": "6.x",
    "flickity": "2.x",
    "hammerjs": "2.x",
    "zone.js": "0.8.x"
  },
